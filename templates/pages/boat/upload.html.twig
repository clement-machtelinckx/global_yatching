{% extends "base.html.twig" %}

{%block title%}Global - Yatching - uload - images {% endblock %}

{% block body %}
<div class="container">

    <h1 class="mt-4">Upload images</h1>

    {{ form_start (form)}}

    <div class="form-group">
        {{ form_label(form.image_name) }}
        <div class="border border-dark">
        {{ form_widget(form.image_name) }}
        </div>
        <div class="form-error">
        {{ form_errors(form.image_name) }}
        </div>
    </div>
    <div class="form-group">
        {{ form_label(form.image_file) }}
        <div class="border border-dark">
        {{ form_widget(form.image_file) }}
        </div>
        <div class="form-error">
        {{ form_errors(form.image_file) }}
        </div>
    </div>
    {{ form_end(form) }}

</div>
{% endblock %}